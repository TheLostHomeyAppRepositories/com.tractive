{
  "title": {
    "en": "GPS location has changed",
    "nl": "GPS-locatie is veranderd"
  },
  "hint": {
    "en": "This flowcard is activated when the the GPS location has changed.",
    "nl": "Deze kaart wordt geactiveerd wanneer de GPS-locatie veranderd is."
  },
  "platforms": [
    "local",
    "cloud"
  ],
  "tokens": [
    {
      "type": "string",
      "name": "street",
      "title": {
        "en": "Street",
        "nl": "Straat"
      },
      "example": {
        "en": "Oxford Street",
        "nl": "Prinsengracht"
      }
    },
    {
      "type": "string",
      "name": "house_number",
      "title": {
        "en": "House Number",
        "nl": "Huisnummer"
      },
      "example": {
        "en": "300",
        "nl": "252"
      }
    },
    {
      "type": "string",
      "name": "city",
      "title": {
        "en": "City",
        "nl": "Plaats"
      },
      "example": {
        "en": "London",
        "nl": "Amsterdam"
      }
    },
    {
      "type": "string",
      "name": "zip_code",
      "title": {
        "en": "Zip Code",
        "nl": "Postcode"
      },
      "example": {
        "en": "W1C 1",
        "nl": "1016 HG"
      }
    },
    {
      "type": "string",
      "name": "country",
      "title": {
        "en": "Country code",
        "nl": "Landcode"
      },
      "example": {
        "en": "GB",
        "nl": "NL"
      }
    }
  ],
  "args": [
    {
      "type": "device",
      "name": "device",
      "filter": "driver_id=tracker"
    }
  ]
}
